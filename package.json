{
  "name": "posq-restaurant-pos",
  "version": "1.0.0",
  "description": "Full Restaurant POS + QR Ordering System",
  "main": "server/src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd server && npm start",
    "test": "cd server && npm test",
    "migrate": "cd server && npm run migrate",
    "seed": "cd server && npm run seed",
    "setup": "npm run migrate && npm run seed"
  },
  "keywords": ["pos", "restaurant", "qr-ordering", "pwa"],
  "author": "POSQ Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}